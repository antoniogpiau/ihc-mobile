<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.puc.ihc.capital" >

    <application
        android:allowBackup="true"
        android:icon="@drawable/logo50"
        android:label="@string/app_name"
        android:theme="@style/AppTheme" >

        <activity
            android:name=".MainActivity"
            android:label="@string/app_name" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />
                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <activity
            android:name=".LoginActivity"
            android:label="@string/title_activity_login" >
        </activity>
        <activity
            android:name=".TokenActivity"
            android:label="@string/title_activity_token" >
        </activity>
        <activity
            android:name=".ContatoActivity"
            android:label="@string/title_activity_contato" >
        </activity>
        <activity
            android:name=".AgenciasActivity"
            android:label="@string/title_activity_agencias" >
        </activity>
        <activity
            android:name=".PrincipalActivity"
            android:label="@string/title_activity_main" >
        </activity>
    </application>

</manifest>
