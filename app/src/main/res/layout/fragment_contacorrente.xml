<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools" android:layout_width="match_parent"
    android:layout_height="match_parent" android:paddingLeft="@dimen/activity_horizontal_margin"
    android:paddingRight="@dimen/activity_horizontal_margin"
    android:paddingTop="@dimen/activity_vertical_margin"
    android:paddingBottom="@dimen/activity_vertical_margin"
    tools:context=".MainActivity$PlaceholderFragment"
    android:id="@+id/relativeLayout1">

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:textAppearance="?android:attr/textAppearanceLarge"
        android:text="Saldo:"
        android:textSize="23dp"
        android:id="@+id/textView2"
        android:layout_alignParentTop="true"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true" />

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:textAppearance="?android:attr/textAppearanceLarge"
        android:text="Limite cheque especial:"
        android:textSize="19dp"
        android:id="@+id/textView5"
        android:layout_below="@+id/textView2"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true"
        android:layout_marginTop="8dp"
        android:layout_marginBottom="5dp"/>

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:textAppearance="?android:attr/textAppearanceLarge"
        android:text="De"
        android:id="@+id/textView3"
        android:layout_alignBaseline="@+id/dataDe"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true" />

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:textAppearance="?android:attr/textAppearanceLarge"
        android:text="AtÃ©"
        android:id="@+id/textView4"
        android:layout_alignBaseline="@+id/dataAte"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true" />

    <EditText
        android:layout_width="100dp"
        android:layout_height="wrap_content"
        android:inputType="date"
        android:ems="10"
        android:id="@+id/dataDe"
        android:layout_marginLeft="10dp"
        android:layout_below="@+id/textView5"
        android:layout_toRightOf="@+id/textView4"
        android:layout_toEndOf="@+id/textView4" />

    <EditText
        android:layout_width="100dp"
        android:layout_height="wrap_content"
        android:inputType="date"
        android:ems="10"
        android:id="@+id/dataAte"
        android:layout_marginLeft="10dp"
        android:layout_below="@+id/dataDe"
        android:layout_toRightOf="@+id/textView4"
        android:layout_toEndOf="@+id/textView4" />



    <ListView
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:id="@+id/listCorrente"
        android:layout_gravity="bottom"
        android:layout_alignParentBottom="true"
        android:layout_alignParentRight="true"
        android:layout_alignParentEnd="true"
        android:headerDividersEnabled="true"
        android:layout_below="@+id/dataAte" />

    <ImageView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/imageDe"
        android:src="@android:drawable/ic_menu_today"
        android:layout_above="@+id/dataAte"
        android:layout_marginBottom="5dp"
        android:layout_toRightOf="@+id/dataDe" />

    <ImageView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/imageAte"
        android:src="@android:drawable/ic_menu_today"
        android:layout_above="@+id/listCorrente"
        android:layout_marginBottom="5dp"
        android:layout_alignLeft="@+id/imageDe"
        android:layout_alignStart="@+id/imageDe" />

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:textAppearance="?android:attr/textAppearanceLarge"
        android:textSize="22dp"
        android:text="R$ 4200.00"
        android:id="@+id/textView29"
        android:layout_toRightOf="@+id/textView4"
        android:textColor="#ff35b200"
        android:layout_marginLeft="40dp" />

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:textSize="18dp"
        android:textAppearance="?android:attr/textAppearanceLarge"
        android:text="R$ 250.00"
        android:id="@+id/textView30"
        android:layout_marginLeft="10dp"
        android:layout_alignBottom="@+id/textView5"
        android:layout_toRightOf="@+id/textView5" />

</RelativeLayout>
